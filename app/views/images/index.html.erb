<% content_for(:navbar) do %>
<li><a href="/">Home</a></li>
<li>
<% if user_signed_in? %>
<%= link_to "Edit Account", edit_user_registration_path %>
<%= link_to "Sign Out", destroy_user_session_path, :method => "delete" %>
<% else %>
<%= link_to "Sign In", user_session_path %>
<% end %>
</li>
<% end %>

<% content_for(:title, "Instagram Clone | Index") %>

<h1>Images</h1>

<ul>
  <% @images.each do |image| %>
    <%= link_to image.user.email, user_session_path(image.user) %>
    <%= image_tag image.url %>
    <li><%= link_to image.caption, image_path(image) %></li>
  <% end %>
</ul>

<%= link_to "New Image", new_image_path %>
